{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "chmod 775 bin/develop.sh && ./bin/develop.sh",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project ."
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "apollo-boost": "^0.4.4",
    "gatsby": "^2.15.28",
    "gatsby-plugin-sass": "^2.1.17",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1.11",
    "gatsby-source-filesystem": "^2.1.28",
    "gatsby-source-graphql": "^2.1.17",
    "gatsby-transformer-remark": "^2.6.26",
    "graphql": "^14.5.8",
    "node-sass": "^4.12.0",
    "react": "^16.10.0",
    "react-apollo": "^3.1.2",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.10.0",
    "react-hover-observer": "^2.1.1",
    "react-markdown": "^4.2.2",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "env-cmd": "^10.0.1",
    "gatsby-source-github": "0.0.2",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
